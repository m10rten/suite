{
  "name": "@mvdlei/zod",
  "version": "0.0.4",
  "description": "Zod Preset and Extension. Includes Schemas, Types, Base interfaces and more.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.10.5",
    "internal-config": "workspace:*",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "zod": "3.22.4"
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./schemas": {
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "schemas": [
        "dist/schemas/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/suite"
  },
  "bugs": {
    "url": "https://github.com/m10rten/suite/issues"
  },
  "dependencies": {
    "@mvdlei/types": "workspace:*"
  }
}
