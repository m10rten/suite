{
  "name": "@mvdlei/tzod",
  "version": "0.0.0",
  "description": "Zod Preset and Extensions. Includes Schemas, Types, Base interfaces and more.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.10.5",
    "internal-config": "workspace:*",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "zod": "3.22.4"
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./t": {
      "import": "./dist/t.mjs",
      "require": "./dist/t.js",
      "default": "./dist/t.js",
      "types": "./dist/t.d.ts"
    },
    "./is": {
      "import": "./dist/is.mjs",
      "require": "./dist/is.js",
      "default": "./dist/is.js",
      "types": "./dist/is.d.ts"
    },
    "./to": {
      "import": "./dist/to.mjs",
      "require": "./dist/to.js",
      "default": "./dist/to.js",
      "types": "./dist/to.d.ts"
    },
    "./string": {
      "import": "./dist/string.mjs",
      "require": "./dist/string.js",
      "default": "./dist/string.js",
      "types": "./dist/string.d.ts"
    },
    "./array": {
      "import": "./dist/array.mjs",
      "require": "./dist/array.js",
      "default": "./dist/array.js",
      "types": "./dist/array.d.ts"
    },
    "./object": {
      "import": "./dist/object.mjs",
      "require": "./dist/object.js",
      "default": "./dist/object.js",
      "types": "./dist/object.d.ts"
    },
    "./number": {
      "import": "./dist/number.mjs",
      "require": "./dist/number.js",
      "default": "./dist/number.js",
      "types": "./dist/number.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "t": [
        "dist/t.d.ts"
      ],
      "is": [
        "dist/is.d.ts"
      ],
      "to": [
        "dist/to.d.ts"
      ],
      "string": [
        "dist/string.d.ts"
      ],
      "array": [
        "dist/array.d.ts"
      ],
      "object": [
        "dist/object.d.ts"
      ],
      "number": [
        "dist/number.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/suite"
  },
  "bugs": {
    "url": "https://github.com/m10rten/suite/issues"
  }
}
