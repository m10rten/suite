{
  "name": "@mvdlei/env",
  "version": "0.0.6",
  "description": "Easy Node Environment Variables, useable in TypeScript and JavaScript on both React and Node applications.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "dev": "pnpm build --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mvdlei/types": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "20.11.2",
    "internal-config": "workspace:*"
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./env": {
      "import": "./dist/env.mjs",
      "require": "./dist/env.js",
      "default": "./dist/env.js",
      "types": "./dist/env.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "env": [
        "dist/env.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/suite"
  },
  "bugs": {
    "url": "https://github.com/m10rten/suite/issues"
  }
}
