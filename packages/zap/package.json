{
  "name": "@mvdlei/zap",
  "version": "0.0.3",
  "description": "A simple fetch wrapper with a focus on type safety and error handling, built on top of zod. Use it like a Zap!",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "dev": "tsup --watch --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.11.2",
    "internal-config": "workspace:*",
    "ts-node": "10.9.2",
    "zod": "3.22.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./zap": {
      "import": "./dist/zap.mjs",
      "require": "./dist/zap.js",
      "types": "./dist/zap.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "zap": [
        "./dist/zap.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@mvdlei/log": "workspace:*",
    "@mvdlei/types": "workspace:*"
  },
  "peerDependencies": {
    "zod": "3.22.4"
  }
}
